<h1>Listing places</h1>

<table>
  <tr>
    <th>Timeline</th>
    <th>User</th>
    <th>Name</th>
    <th>Coordinates</th>
    <th>From start</th>
    <th>From end</th>
    <th>From original</th>
    <th>From resolution</th>
    <th>To start</th>
    <th>To end</th>
    <th>To original</th>
    <th>To resolution</th>
    <th>Description</th>
    <th>Place type</th>
  </tr>

<% for place in @places %>
  <tr>
    <td><%=h place.timeline_id %></td>
    <td><%=h place.user_id %></td>
    <td><%=h place.name %></td>
    <td><%=h place.coordinates %></td>
    <td><%=h place.from_start %></td>
    <td><%=h place.from_end %></td>
    <td><%=h place.from_original %></td>
    <td><%=h place.from_resolution %></td>
    <td><%=h place.to_start %></td>
    <td><%=h place.to_end %></td>
    <td><%=h place.to_original %></td>
    <td><%=h place.to_resolution %></td>
    <td><%=h place.description %></td>
    <td><%=h place.place_type_id %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New place', new_place_path %>
